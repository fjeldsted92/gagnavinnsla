FROM ratings
WHERE movieid = 3535
OR movieid = 20
or movieid = 22

CREATE table threefilms(userid int,movieid int, rating int, timestamp int, PRIMARY KEY (movieid,userid,timestamp,rating),FOREIGN KEY(movieid),REFERENCES(movieid));

CREATE TABLE Threefilms(userid int, movieid int, rating int, time_stamp varchar(250),primary key (movieid ,userid, time_stamp,rating), FOREIGN KEY(movieid) REFERENCES Movies(movieid));

